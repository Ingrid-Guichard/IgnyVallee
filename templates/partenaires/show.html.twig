{% extends 'base.html.twig' %}

{% block title %}Partenaires{% endblock %}

{% block body %}
    <h1>Partenaires</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partenaire.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ partenaire.nom }}</td>
            </tr>
            <tr>
                <th>DatePartenariat</th>
                <td>{{ partenaire.datePartenariat ? partenaire.datePartenariat|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ partenaire.email }}</td>
            </tr>
            <tr>
                <th>LienDuSite</th>
                <td>{{ partenaire.lienDuSite }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ partenaire.description }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ partenaire.imageName }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('partenaires_index') }}">back to list</a>

    <a href="{{ path('partenaires_edit', {'id': partenaire.id}) }}">edit</a>

    {{ include('partenaires/_delete_form.html.twig') }}
